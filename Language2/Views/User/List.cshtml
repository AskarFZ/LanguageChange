@model IEnumerable<Language2.Models.UserModel>

@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Birthday)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@for (int i=0;i< ((List<UserModel>)Model).Count;i++) {
            UserModel item = ((List<UserModel>)Model)[i];
            <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Birthday)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  index=i }) |
                @Html.ActionLink("Delete", "Delete", new {  index=i })
            </td>
        </tr>
}
    </tbody>
</table>
